<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/top1layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:id="@+id/contentLeft"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/sub_top_liist_bg"
                android:orientation="vertical"
                android:padding="16dp" >

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dip"
                    android:layout_weight="1" >

                    <ImageView
                        android:id="@+id/top_img1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />

                    <TextView
                        android:id="@+id/top_textview1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:maxLines="5"
                        android:padding="10dip"
                        android:textSize="17sp"
                        android:visibility="gone" />
                </FrameLayout>

                <TextView
                    android:id="@+id/top_title1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="2"
                    android:minLines="2"
                    android:textColor="#000"
                    android:textSize="15sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp" >

                    <TextView
                        android:id="@+id/heart_count"
                        style="@style/content_icos"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawableLeft="@drawable/ico_list_heart"
                        android:text="0" />

                    <TextView
                        android:id="@+id/reply_count"
                        style="@style/content_icos"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawableLeft="@drawable/ico_list_reply"
                        android:text="0" />

                    <TextView
                        android:id="@+id/scrap_count"
                        style="@style/content_icos"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawableLeft="@drawable/ico_list_scrap"
                        android:maxLines="5"
                        android:text="0" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/top2layout"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/sub_top_liist_bg"
                android:orientation="vertical"
                android:padding="16dp" >

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dip"
                    android:layout_weight="1" >

                    <ImageView
                        android:id="@+id/top_img2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop" />

                    <TextView
                        android:id="@+id/top_textview2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="10dip"
                        android:textSize="17sp"
                        android:visibility="gone" />
                </FrameLayout>

                <TextView
                    android:id="@+id/top_title2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="2"
                    android:minLines="2"
                    android:textColor="#000"
                    android:textSize="15sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp" >

                    <TextView
                        android:id="@+id/heart2_count"
                        style="@style/content_icos"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawableLeft="@drawable/ico_list_heart"
                        android:text="0" />

                    <TextView
                        android:id="@+id/reply2_count"
                        style="@style/content_icos"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawableLeft="@drawable/ico_list_reply"
                        android:text="0" />

                    <TextView
                        android:id="@+id/scrap2_count"
                        style="@style/content_icos"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:drawableLeft="@drawable/ico_list_scrap"
                        android:text="0" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

